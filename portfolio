css code
* {
   margin: 0;
   padding: 0;
   font-family:sans-serif;
}

/* Styles for header section */
.header {
   width: 100%;
   height: 100vh;
   background-color: lightblue;
}

/* Styles for navigation bar */
nav {
   display: flex;
   margin: auto;
   width: 90%;
   padding: 20px;
   align-items: center;
   justify-content: space-between;
}

nav ul li {
   display: inline-block;
   list-style: none;
   margin: 10px;
}

nav ul li a {
   text-decoration: none;
   color: black;
   font-weight: bolder;
}

nav ul li a:hover {
   background-color: seagreen;
   border-radius: 2px;
   color: white;
}

.button-cv, .button-gfc{
   display: inline-block;
   margin-left: 0%;
